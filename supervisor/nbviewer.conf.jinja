[program:nbviewer]
command=/usr/share/nbviewer/venv/bin/python -m nbviewer --port=8080 --threads=8
numprocs=1
numprocs_start=0
process_name=%(program_name)s
autostart=true
autorestart=true
startsecs=1
startretries=3
exitcodes=0,2
stopsignal=QUIT
stopwaitsecs=10
user=nobody
stdout_logfile=/var/log/nbviewer
directory=/usr/share/nbviewer
environment=HOME="/usr/share/nbviewer",USER="nobody",PATH="/usr/share/nbviewer/venv/bin:/usr/local/bin:/usr/bin:/bin:/usr/local/games:/usr/games",VIRTUAL_ENV="/usr/share/nbviewer/venv",DATABASE_URL="sqlite:///:memory:",IPYTHONDIR="/tmp/ipython-garbage",MEMCACHE_SERVERS=127.0.0.1{{ environment }}
redirect_stderr=false
stdout_logfile=AUTO
stdout_logfile_maxbytes=50MB
stdout_logfile_backups=10
stdout_capture_maxbytes=0
stdout_events_enabled=true
stderr_logfile=AUTO
stderr_logfile_maxbytes=50MB
stderr_logfile_backups=10
stderr_capture_maxbytes=0
stderr_events_enabled=true
serverurl=AUTO
