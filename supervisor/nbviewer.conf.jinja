[program:nbviewer]
command=/usr/share/nbviewer/venv/bin/newrelic-admin run-python -m nbviewer --port=8080 --processes=8 --mc_threads=4
numprocs=1
numprocs_start=0
process_name=%(program_name)s
autostart=true
autorestart=true
startsecs=1
startretries=3
exitcodes=0,2
stopsignal=QUIT
stopwaitsecs=10
user=nobody
directory=/usr/share/nbviewer
environment=HOME="/usr/share/nbviewer",USER="nobody",PATH="/usr/share/nbviewer/venv/bin:/usr/local/bin:/usr/bin:/bin:/usr/local/games:/usr/games",VIRTUAL_ENV="/usr/share/nbviewer/venv",DATABASE_URL="sqlite:///:memory:",IPYTHONDIR="/tmp/ipython-garbage",MEMCACHE_SERVERS="127.0.0.1",NEW_RELIC_CONFIG_FILE={{ new_relic_config_file }}{{ environment }}

redirect_stderr=true

stdout_logfile=/var/log/nbviewer
stdout_logfile_maxbytes=50MB
stdout_logfile_backups=10
stdout_capture_maxbytes=0
stdout_events_enabled=true

serverurl=AUTO
